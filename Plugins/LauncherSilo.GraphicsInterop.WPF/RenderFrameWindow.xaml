<Window x:Class="LauncherSilo.GraphicsInterop.WPF.RenderFrameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:LauncherSilo.GraphicsInterop.WPF"
        Loaded="Window_Loaded"
        SizeChanged="Window_SizeChanged"
        mc:Ignorable="d" 
        d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <local:GraphicsInteropPanel>
            <local:GraphicsInteropEllipse></local:GraphicsInteropEllipse>
            <local:GraphicsInteropLine></local:GraphicsInteropLine>
            <local:GraphicsInteropRectangle></local:GraphicsInteropRectangle>
        </local:GraphicsInteropPanel>
        <Image x:Name="TargetImage" Stretch="Fill" ></Image>
    </Grid>
</Window>
